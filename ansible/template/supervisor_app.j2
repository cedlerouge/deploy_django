[program:{{ item.name}}]
command=/opt/.virtualenv/{{item.name}}/env/bin/gunicorn betwit.wsgi:application -b 0.0.0.0:{{ item.port }}
stdout_logfile=/var/log/supervisor/{{ item.name }}.log
stderr_logfile=/var/log/supervisor/{{ item.name }}.error
autostart=true
autorestart=true
directory=/opt/{{ item.name }}/
